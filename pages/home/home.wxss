

  .container{
    width: 100%;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    height: 100vh;
    overflow: hidden;
    background-color: #000;
  }
  .home_bg {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
  }
  
  .text{
    width: 200rpx;
    height: 80rpx;
    display:flex;
    justify-content: center;
    align-items: center;
    background-color: rgb(189, 93, 9);
    z-index: 2;
    border-radius: 10rpx;
    border: 1rpx solid #fff;
    position: relative;
    color: #fff;
    font-weight: normal;
    font-size: 48rpx;
    font-weight: 800;
    line-height: 1;
    margin-top: 400rpx;
    /* animation: split 5s steps(18) infinite ; */
  }
